class Solution {
public:
	vector<int>twoSum(vector<int>& nums, int target) {
		unorderd_map<int, int>record;
    for(int i=0;i<nums.size();i++){
    int num=target-nums[i];
    if(record.find(num )!=record.end()){
    return vector<int>{record[num],i};
    }
    record[nums{i]]=i
    }
return vector<int>{-1,-1};
	}
};
